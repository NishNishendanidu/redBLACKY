{

  "name": "Red Blacky",

  "description": "This app does one little thing, and does it well.",

  "keywords": [

    "productivity",

    "HTML5",

    "scalpel"

  ],

  "website": "https://small-sharp-tool.com/",

  "repository": "https://github.com/NishNishendanidu/redBLACKY",

  "logo": "https://i.ibb.co/K9Sx4Cf/Red-Blacky-Logo.jpg",

  "success_url": "/welcome",

  "scripts": {

  "postdeploy": "bundle exec rake bootstrap"

  },

  "env": {

    "RED_BLACKY_SESSION": {

      "description": "A secret key for verifying the integrity of signed cookies.",

      "value" "type redbkacky session" 
      
      
      
      "NAME_OWNER"
      
      "description": "type owner name" 
      
      "value":"nishen" 
      
      
      
      "Alive_massege" 
      
      "description": "alive logo link" 
      
      "value":"https://i.ibb.co/K9Sx4Cf/Red-Blacky-Logo.jpg" 
      
      
      
      "welcome_massege"
      
      "description":"link welcome massege"
      
      "value":"https://i.ibb.co/K9Sx4Cf/Red-Blacky-Logo.jpg" 
      

    },

    "WEB_CONCURRENCY": {

      "description": "The number of processes to run.",

      "value": "5"

    }

  },

  "formation": {

    "web": {

      "quantity": 1,

      "size": "standard-1x"

    }

  },

  "image": "heroku/ruby",

  "addons": [

    "openredis",

    {

      "plan": "mongolab:shared-single-small",

      "as": "MONGO"

    },

    {

      "plan": "heroku-postgresql",

      "options": {

        "version": "9.5"

      }

    }

  ],

  "buildpacks": [

    {

      "url": "https://github.com/stomita/heroku-buildpack-phantomjs"

    }

  ],

  "environments": {

    "test": {

      "scripts": {

        "test": "bundle exec rake test"

      }

    }

  }

}
